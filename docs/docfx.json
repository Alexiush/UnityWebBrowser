 
{
    "metadata": [
        {
            "src": [
                {
                    "src": "../src/Packages/UnityWebBrowser",
                    "files": [
                        "**/*.cs"
                    ]
                },
                {
                    "src": "../src/VoltstroStudios.UnityWebBrowser.Shared",
                    "files": [
                        "VoltstroStudios.UnityWebBrowser.Shared.csproj"
                    ]
                }
            ],
            "globalNamespaceId": "Global",
            "filter": "filterConfig.yml",
            "dest": "api",
            "allowCompilationErrors": true
        }
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "UnityWebBrowser",
            "_appFooter": "UnityWebBrowser"
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md",
                    "articles.md"
                ]
            },
            {
                "src": "../",
                "files": [
                    "CHANGELOG.md"
                ]
            },
            {
                "src": "api",
                "files": [
                    "*.yml",
                    "*.md"
                ],
                "dest": "api"
            },
            {
                "src": "articles",
                "files": [
                    "toc.yml",
                    "**/*.md"
                ],
                "dest": "articles"
            }
        ],
        "resource": [
            {
                "files": [
                    "assets/**/*"
                ]
            }
        ],
        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/2021.3/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "_site"
    }
}